{
    "containerDefinitions": [
        {
            "essential": true,
            "name": "app"
        },
        {
            "essential": true,
            "image": "nginx:latest",
            "mountPoints": [
                {
                    "containerPath": "/etc/nginx/conf.d",
                    "sourceVolume": "_NginxConf_D"
                }
            ],
            "name": "nginx",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                },
                {
                    "containerPort": 443,
                    "hostPort": 443
                }
            ]
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "./nginx/conf.d"
            },
            "name": "_NginxConf_D"
        }
    ]
}